{
  "name": "mfai-mcp-aisdk-test",
  "version": "1.0.0",
  "description": "Test client for MFAI MCP server using Vercel AI SDK",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:sse": "vitest run src/tests/connection/sse.test.ts",
    "test:tools": "vitest run src/tests/tools/discovery.test.ts",
    "test:integration": "vitest run src/tests/integration/*.test.ts",
    "example:basic": "tsx src/examples/basic-usage.ts",
    "example:ai": "tsx src/examples/ai-repository-analysis.ts",
    "benchmark": "tsx scripts/benchmark.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "ai": "^3.0.0",
    "@ai-sdk/google": "^0.0.1",
    "zod": "^3.22.0",
    "dotenv": "^16.4.0",
    "eventsource": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/eventsource": "^1.1.0",
    "typescript": "^5.3.0",
    "tsx": "^4.0.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
}